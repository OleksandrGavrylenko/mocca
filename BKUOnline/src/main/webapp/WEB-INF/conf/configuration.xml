<?xml version="1.0" encoding="UTF-8"?>
<MoccaConfiguration version="1.0">
  <DataURLConnection>
		<!--
			The maximum number of consecutive Security Layer requests received
			from DataURL server before execution is aborted.
		-->
    <MaxHops>50</MaxHops>
  </DataURLConnection>
  <!-- default revocation service type order: OCSP, CRL
  <SSL>
    <revocationServiceOrder>OCSP</revocationServiceOrder>
  </SSL-->
</MoccaConfiguration>